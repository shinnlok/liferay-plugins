<?xml version="1.0"?>

<ivy-module
	version="2.0"
	xmlns:m2="http://ant.apache.org/ivy/maven"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd"
>
	<info module="google-drive-hook" organisation="com.liferay">
		<extends extendType="configurations,description,info" location="${sdk.dir}/ivy.xml" module="com.liferay.sdk" organisation="com.liferay" revision="latest.integration" />
	</info>

	<dependencies defaultconf="default">
		<dependency name="google-api-client" org="com.google.api-client" rev="1.17.0-rc" />
		<dependency name="google-api-services-drive" org="com.google.apis" rev="v2-rev111-1.17.0-rc" />
		<dependency name="google-http-client" org="com.google.http-client" rev="1.17.0-rc" />
		<dependency name="google-http-client-jackson2" org="com.google.http-client" rev="1.17.0-rc" />
		<dependency name="google-oauth-client" org="com.google.oauth-client" rev="1.17.0-rc" />
		<dependency name="jackson-core" org="com.fasterxml.jackson.core" rev="2.1.3" />
	</dependencies>
</ivy-module>